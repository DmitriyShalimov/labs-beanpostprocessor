<beans>

    <bean id="massagePrinter" class="ua.shalimov.studyofioc.printer.MassagePrinter">
        <property name="massage" value="${massage}"/>
    </bean>
    <bean id="propertyPlaceholder" class="ua.shalimov.studyofioc.beanfactorypostprocessor.PropertyPlaceholder">
    </bean>
    <bean id="transactionalBeanPostProcessor" class="ua.shalimov.studyofioc.beanpostprocessor.TransactionalBeanPostProcessor">
    </bean>
    <bean id="injectValueBeanPostProcessor" class="ua.shalimov.studyofioc.beanpostprocessor.InjectValueBeanPostProcessor">
    </bean>
    <bean id="loggerBeanPostProcessor" class="ua.shalimov.studyofioc.beanpostprocessor.LoggerBeanPostProcessor">
    </bean>
</beans>